{
  "version": 2,
  "buildCommand": "cd ../.. && npm install && npm run build:shared && cd services/auth-service && npm run build",
  "installCommand": "cd ../.. && npm install",
  "outputDirectory": "dist",
  "framework": null,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/index.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}

