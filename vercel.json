{
    "buildCommand": "cd dashboard && npm run build",
    "outputDirectory": "dashboard/.next",
    "installCommand": "npm install --legacy-peer-deps && cd dashboard && npm install --legacy-peer-deps",
    "framework": "nextjs",
    "regions": [
        "iad1"
    ],
    "functions": {
        "dashboard/app/api/**/*.ts": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "env": {
        "NODE_ENV": "production"
    }
}